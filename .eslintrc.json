{
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "@typescript-eslint", 
    "prettier"
  ],
  "env": {
    "jest": true,
    "es6": true
  },
  "rules": {
    "no-undef": "off"
  }
}